From: Mike Hommey <mh@glandium.org>
Date: Fri, 25 Feb 2011 13:43:19 +0100
Subject: Remove -ljs_static from test linking flags

This is part of https://bugzilla.mozilla.org/show_bug.cgi?id=584474
---
 config/rules.mk        |    2 +-
 js/src/config/rules.mk |    2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/config/rules.mk b/config/rules.mk
index 3675b8b..f97a14d 100644
--- a/config/rules.mk
+++ b/config/rules.mk
@@ -224,7 +224,7 @@ ifdef CPP_UNIT_TESTS
 CPPSRCS += $(CPP_UNIT_TESTS)
 SIMPLE_PROGRAMS += $(CPP_UNIT_TESTS:.cpp=$(BIN_SUFFIX))
 INCLUDES += -I$(DIST)/include/testing
-LIBS += $(XPCOM_GLUE_LDOPTS) $(NSPR_LIBS) -ljs_static
+LIBS += $(XPCOM_GLUE_LDOPTS) $(NSPR_LIBS)
 
 # ...and run them the usual way
 check::
