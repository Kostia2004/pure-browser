From: Mike Hommey <mh@glandium.org>
Date: Fri, 26 Feb 2010 16:25:15 +0100
Subject: [PATCH 38/45] Install js shell when running make install from js/src

---
 js/src/Makefile.in |    2 +-
 1 files changed, 1 insertions(+), 1 deletions(-)

diff --git a/js/src/Makefile.in b/js/src/Makefile.in
index 826a093..4aa6342 100644
--- a/js/src/Makefile.in
+++ b/js/src/Makefile.in
@@ -622,7 +622,7 @@ endif
 install:: $(filter-out nanojit/%,$(INSTALLED_HEADERS))
 	$(SYSINSTALL) $(IFLAGS1) $^ $(DESTDIR)$(includedir)/$(MODULE)
 
-install:: $(SCRIPTS)
+install:: $(SCRIPTS) $(SIMPLE_PROGRAMS)
 	$(SYSINSTALL) $(IFLAGS2) $^ $(DESTDIR)$(bindir)
 
 install:: $(LIBRARY) $(SHARED_LIBRARY) $(IMPORT_LIBRARY)
